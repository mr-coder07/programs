def insertNodeAtPosition(llist, data, position):
    # Write your code here
    nwnode = SinglyLinkedListNode(data)
    ptr = llist
    i=0
    while i!=position -1 :
        ptr = ptr.next
        i+=1
    nwnode.next = ptr.next
    ptr.next=nwnode
    return llist

