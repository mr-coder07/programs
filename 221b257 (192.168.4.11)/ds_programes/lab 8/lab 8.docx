def deleteNode(llist, position):
    # Write your code here
    if llist is None :
        return llist
    if position == 0:
        llist = llist.next
    ptr = llist
    i=0
    while i!=position-1 and ptr.next is not None:
        ptr=ptr.next
        i+=1
    if ptr.next is not None:
        ptr.next=ptr.next.next
    return llist

